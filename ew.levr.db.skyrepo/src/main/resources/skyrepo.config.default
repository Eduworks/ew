urlBase=#add(
	a="http://localhost:9200"
);
#urlBase=urlBase;

adminPpk=#fileLoad(path="skyAdmin.pem").fileToString();
adminPpk=#if(
	operator=#fileExists(path="skyAdmin.pem"),
	operand="false",
	eq=#object(
		a=#fileSave(obj=#rsaGenerate(),path="skyAdmin.pem"),
		b=adminPpk
	).cget(b=""),
	ne=adminPpk
).cache(name="adminPpk");

#skyIdAdminPpk = adminPpk;